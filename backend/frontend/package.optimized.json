    "echarts": "^6.0.0",
    "element-plus": "^2.13.2",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^3.2.1",
    "vue": "^3.5.25",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.16",
