{
    "name": "ip-manager/tests",
    "description": "IP管理器测试套件",
    "type": "project",
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "../backend/core/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "./"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:unit": "phpunit --testsuite=Unit",
        "test:integration": "phpunit --testsuite=Integration",
        "test:coverage": "phpunit --coverage-html coverage"
    }
}
